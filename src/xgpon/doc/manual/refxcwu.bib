@ARTICLE{wang05tcpwestwooda,
  author = {Ren Wang and M.Yahya Sanadidi and Mario Gerla},
  title = {TCP with sender-side intelligence to handle dynamic, large, leaky
	pipes},
  journal = {IEEE Journal on Selected Areas in Communications},
  year = {2005},
  volume = {23},
  pages = {235-248},
  keywords = {TCP; westwood; congestion control; LFN; large BDP; lossy},
  owner = {xw2},
  review = {An enhancement of TCP westwood for dynamic, large,leaky pipe.
	
	Use rate estimation and agile probing to solve slow start overshoot
	(compared with --- hoe96tcpstartup);
	
	Agile probing and Persistent Non-congestion Detection for probe sudden
	bandwidth increase during CA.
	
	For rate estimation, the sample interval is increased with queue delay.
	Interval = rtt * (cwnd/rttmin - cwnd/srtt)/(cwnd/rttmin)
	
	When network is underutilized and the interval is small, the bandwidth
	estimation acts like "packet pair" and the estimation is close to
	link capacity. 
	
	Hence with agile probing (keep setting ssthresh based on the estimation),
	the sender can probe network capacity quickly in slow start.
	
	When network is congested (end of slow start), the sample interval
	is long. 
	
	The bandwidth estimation acts like "packet train" and the estimation
	is close to real received rate. 
	
	Hence, overshoot will not happen at the end of slow start.
	
	Persistent Non-congestion Detection is carried out during CA. 
	
	When it is detected, agile probing is used to probe the suddenly increased
	bandwidth.},
  timestamp = {2009.11.18}
}

