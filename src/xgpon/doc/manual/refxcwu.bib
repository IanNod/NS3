@ARTICLE{liu01redmarkfront,
  author = {Chunlei Liu and Raj Jain},
  title = {Improving explicit congestion notification with the mark-front strategy},
  journal = {Computer Networks},
  year = {2001},
  volume = {35},
  pages = {185-201},
  keywords = {Internet; queue management scheme; RED; mark-front;},
  owner = {xw2},
  review = {It's a modification of RED.
	
	RED normally marks the packet at the tail of buffer.
	
	This one mark packet at the head so that the receiver, hence the sender
	can get the congestion signal earlier.
	
	Average queue length can be shorter.
	
	Chunlei Liu, Raj Jain: Approaches of Wireless TCP Enhancement and
	A New Proposal Based on Congestion Coherence. HICSS 2003
	
	Another paper from the same authors. They assume that ECN is widely
	adopted---unlikely.
	
	When receiver (mobile host) detects segment loss, if ECN bit has not
	been received, it waits a while for link retransmission. Otherwise,
	3rd dupack is sent immediately.
	
	When sender (server) receives 3rd dupack, if ECN bit has not been
	eceived, it waits a while for new ack. Otherwise, cwnd is reduced
	immediately.},
  timestamp = {2009.11.18}
}

