@CONFERENCE{goff00freezetcp,
  author = {Tom Goff and James Moronski and D. S. Phatak and Vipul Gupta},
  title = {Freeze-TCP: A true end-to-end TCP enhancement mechanism for mobile
	environments},
  booktitle = {INFOCOM},
  year = {2000},
  keywords = {TCP; performance; wireless network; mobility; handoff; freeze-TCP},
  owner = {xw2},
  review = {Freeze-TCP is an end-to-end proposal to handle frequently disconnections
	in wireless networks. It is not designed for the high BER of wireless
	networks.
	
	The mobile client must monitor wireless link quality (SNR), predict
	the coming disconnection and/or handoff (It may be a little hard
	in reality; wrong judgment). 
	
	Then the mobile client sends Zero-Window-ACK in order to stop the
	sender to send. 
	
	The ahead time should be about one RTT (Hard to achieve this). 
	
	When the connection recovered, Freeze-TCP uses three dup-acks to trigger
	the sender begin to send in full speed.
	
	There is an assumption that ABWs before and after disconnection are
	close to each other. 
	
	It may be wrong in some cases. For example, if a PDA moves from one
	WLAN to another one, the ABWs may differ very much due to different
	contention (load) situation of the two channels.
	
	an end-to-end only receiver-changed mechanism for handoff;
	
	Mobile host need predict handoff and transmit "zero window" before
	old link is broken, to Freeze TCP cwnd and ssthresh. "quite hard
	to predict"
	
	Mobile host then send three dupack immediately after handoff completes.
	
	Freeze TCP sender will begin to send with the rate before handoff.
	
	Freeze TCP can not work with high speed wireless network, too aggressive.
	
	It also violate TCP standard that the sender should set cwnd to one
	after a long idle time.},
  timestamp = {2009.11.18}
}

