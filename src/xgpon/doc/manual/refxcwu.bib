@ARTICLE{ding08tcpvegasmultihop,
  author = {Lianghui Ding and Xinbing Wang and Youyun Xu and Wenjun Zhang},
  title = {Improve Throughput of TCP-Vegas in Multihop Ad Hoc Networks},
