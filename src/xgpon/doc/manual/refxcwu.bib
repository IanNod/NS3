@CONFERENCE{abrantes08flashcrowdRCP,
  author = {Filipe Abrantes and Joao Taveira Araujo and Manuel Ricardo},
  title = {Flash Crowd Effect in RCP},
  booktitle = {PFLDnet Workshop},
  year = {2008},
  keywords = {congestion control; high speed network; network-based approach; RCP;
	flash crowd},
  owner = {xw2},
  review = {This paper evaluate analyzes RCP performance when flash crowd flows
	come.
	
	RCP is a congestion control with explicit congestion notification.
	
	The router calculates a common rate for all and allow new flows to
	adopt this rate immediately.
	
	Hence, when new flow arrives, network will be overloaded and queue
	will be built up.
	
	The system will be stabalized after the router find the correct N,
	the number of flows.
	
	During flash crowd, flows come in a short time and system may be not
	able to stabalize.
	
	This paper uses fluid model to investigae this problem.
	
	They use different parameters' value (RCP) and different flow arrival
	pattern/speed to investigate 
	
	in which case RCP can tolerate which load of flash crowd and the system
	can be stabalized again.},
  timestamp = {2009.11.18}
}

