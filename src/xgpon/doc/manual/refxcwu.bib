@CONFERENCE{altman05mimdfairness,
  author = {E. Altman and K. E. Avrachenkov and B. J. Prabhu},
  title = {Fairness in MIMD Congestion Control Algorithms},
  booktitle = {INFOCOM},
  year = {2005},
  keywords = {MIMD; fairness; analysis; markov chain;},
  owner = {xw2},
  review = {Using Markov chain, the authors analyzed the fairness issues of MIMD.
	
	in-protocol fairness and coexistence with AIMD.
	
	They get some results that are very straitforword. They can be guessed
	intuitively.
	
	1. in the presence of rate dependent losses, the capacity is fairly
	shared between the two
	
	sessions whereas rate independent asynchronous losses result in high
	unfairness even when sessions are symmetric.
	
	2. several sessions, each of which uses either the AIMD or the MIMD
	algorithm, share a bottleneck link.
	
	With the assumption rate-independent loss, AIMD session obtains a
	share which is independent of the link capacity.
	
	The reason is that MIMD makes the interval between two consecutive
	packet loss is a constant. C*RTT.
	
	For AIMD(1, 0.5) flow, cwnd/2 = C. Hence, it's throughput is fixed
	no matter capacity value.},
  timestamp = {2009.11.18}
}

